    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="grade" type="Integer" array="False" size=""/>
            <output expression="&quot;please enter you're grade.&quot;" newline="True"/>
            <input variable="grade"/>
            <if expression="grade &gt;=95">
                <then>
                    <output expression="&quot;4.0&quot;" newline="True"/>
                    <output expression="&quot;Outstanding!&quot;" newline="True"/>
                </then>
                <else>
                    <if expression="grade&gt;=91">
                        <then>
                            <output expression="&quot;3.5&quot;" newline="True"/>
                            <output expression="&quot;Excellent!&quot;" newline="True"/>
                        </then>
                        <else>
                            <if expression="grade &gt;=87">
                                <then>
                                    <output expression="&quot;3.0&quot;" newline="True"/>
                                    <output expression="&quot;You made It!&quot;" newline="True"/>
                                </then>
                                <else>
                                    <if expression="grade &gt;=83">
                                        <then>
                                            <output expression="&quot;2.5&quot;" newline="True"/>
                                            <output expression="&quot;You Did Great!&quot;" newline="True"/>
                                        </then>
                                        <else>
                                            <if expression="grade &gt;=79">
                                                <then>
                                                    <output expression="&quot;2.0&quot;" newline="True"/>
                                                    <output expression="&quot;GOOD JOB&quot;" newline="True"/>
                                                </then>
                                                <else>
                                                    <if expression="grade &gt;=75">
                                                        <then>
                                                            <output expression="&quot;1.5&quot;" newline="True"/>
                                                            <output expression="&quot;You Passed!&quot;" newline="True"/>
                                                        </then>
                                                        <else>
                                                            <if expression="grade &gt;=70">
                                                                <then>
                                                                    <output expression="Grade = 1.0" newline="True"/>
                                                                    <output expression="&quot;You nearly made it.&quot;" newline="True"/>
                                                                </then>
                                                                <else>
                                                                    <if expression="grade &lt;=70">
                                                                        <then>
                                                                            <output expression="&quot;R&quot;" newline="True"/>
                                                                            <output expression="&quot;You Failed.&quot;" newline="True"/>
                                                                        </then>
                                                                        <else/>
                                                                    </if>
                                                                </else>
                                                            </if>
                                                        </else>
                                                    </if>
                                                </else>
                                            </if>
                                        </else>
                                    </if>
                                </else>
                            </if>
                        </else>
                    </if>
                </else>
            </if>
        </body>
