    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="who" type="Integer" array="False" size=""/>
            <declare name="guess" type="Integer" array="False" size=""/>
            <assign variable="who" expression="random(300) +1"/>
            <output expression="&quot;please enter a guess between 1 and 300&quot;"/>
            <do expression="guess != who">
                <input variable="guess"/>
                <if expression="guess&gt;who">
                    <then>
                        <output expression="&quot;your guess is too High&quot;"/>
                    </then>
                    <else/>
                </if>
                <if expression="guess&lt;who">
                    <then>
                        <output expression="&quot;your guess is too LOW&quot;"/>
                    </then>
                    <else/>
                </if>
            </do>
            <output expression="&quot;correct!&quot;"/>
        </body>
    </function>
</flowgorithm>
